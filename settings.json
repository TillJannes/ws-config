{
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "workbench.editorAssociations": {"*.pdf": "latex-workshop-pdf-hook"},

  "workbench.startupEditor": "none",
  "workbench.settings.applyToAllProfiles": [],

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  
  "files.autoSave": "afterDelay",
  "chat.editor.wordWrap": "on",
  "settingsSync.ignoredSettings": [],

  "python.defaultInterpreterPath": "/Applications/anaconda3/envs/esnProject/bin/python",
  "python.terminal.activateEnvironment": false,
  "[python]": {"editor.formatOnType": true},
  "python.analysis.packageIndexDepths": [
  {
    "name": "sklearn",
    "depth": 2},
  {
    "name": "matplotlib",
    "depth": 2},
  {
    "name": "scipy",
    "depth": 2},
  {
    "name": "django",
    "depth": 2},
  {
    "name": "flask",
    "depth": 2},
  {
    "name": "fastapi",
    "depth": 2}],

  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "%DOC%"]},
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"]},
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}}],
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.autoBuild.interval": 5000,
  "latex.linter.enabled": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.message.error.show": false,
  "latex-workshop.message.badbox.show": false,
  "latex-workshop.linting.chktex.enabled": false,
  "latex-workshop.linting.chktex.exec.args": ["-wall", "-n22", "-n30","-e16", "-q", "-n3", "-n12", "-n35", "-n46"],
  "latex-workshop.synctex.synctexjs.enabled": true,
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.view.pdf.viewer": "tab"}